{% extends "base.html.twig" %}

{% block content %}
    <h1>{{ content.title }}</h1>

    {{ content.article|raw }}

    <ul class="events">
        {% for event in content.events %}
            <li><a href="{{ path('app.event', {id: event.id}) }}">{{ event.title }}</li></a>
        {% endfor %}
    </ul>
{% endblock %}
